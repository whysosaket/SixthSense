{
  "version": 2,
  "name": "sixthsense",
  "builds": [
     {
        "src": "index.js",
        "use": "@vercel/node"
     }
  ],
  "routes": [
     {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": [
           "GET",
           "POST",
           "PUT",
           "DELETE",
           "PATCH",
           "OPTIONS"
        ],
        "headers": {
           "Access-Control-Allow-Origin": "*"
        }
     }
  ]
}